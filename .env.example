## .env.example

# PostgreSQL Database Configuration
POSTGRES_USER=admin
POSTGRES_PASSWORD=your_db_password
POSTGRES_DB=wheelOfLifeDB

# Spring Boot Configuration
SPRING_DATASOURCE_URL=jdbc:postgresql://localhost:5432/wheelOfLifeDB
SPRING_DATASOURCE_USERNAME=admin
SPRING_DATASOURCE_PASSWORD=your_db_password
SPRING_JPA_HIBERNATE_DDL_AUTO=update
SPRING_JPA_SHOW_SQL=true

# JWT Secret (Use a securely generated value in production)
JWT_SECRET=your_secure_jwt_secret_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_API_BASE=https://api.openai.com/v1
OPENAI_MODEL=gpt-4.1-nano

# URLs for Microservices (adjust if deployed externally)
VITE_SERVER_URL="http://localhost:8080"
VITE_AUTH_URL="http://localhost:8081"
VITE_GENAI_URL="http://localhost:5001"
DATABASE_URL=postgresql://admin:admin@db:5432/wheelOfLifeDB

# Grafana Credentials
GRAFANA_USER=admin
GRAFANA_PASSWORD=your_grafana_password
